// Entity Electricity
@DbTable(Electricity)
Electricity {
    FirstName : string { #TextInput };
    LastName : string { #TextInput };
    document : string { #UploadInput };
    passport : string { #UploadInput };
    idDocument : string { #UploadInput };
    phoneNumber : string { #TextInput };
    email : string { #TextInput };
    description : string { #TextInput };
}
